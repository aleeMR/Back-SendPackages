{
    "version": 2,
    "builds": [
        {
            "src": "./src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/package/(.*)",
            "dest": "./src/index.js"
        }
    ],
    "rewrites": [
        {
            "source": "/api/package/(.*)",
            "destination": "./src/index.js"
        }
    ]
}